{
  "hosting": {
    "site": "ismaaa-natours",
    "public": "./functions/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "!./functions/config.env"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31557600, s-maxage=31557600"
          }
        ]
      }
    ]
  }
}